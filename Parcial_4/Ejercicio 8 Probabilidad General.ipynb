   "source": [
    "import numpy as np\n",
    "N=1000000\n",
    "pp=0\n",
    "moneda=np.array([-1,1])\n",
    "for i in range(N):\n",
    "    lanzamiento=np.random.choice(moneda,4)\n",
    "    if np.sum(lanzamiento)==0:\n",
    "        pp+=1\n",
    "pp/N\n"
   ]
  }
